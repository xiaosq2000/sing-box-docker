{
    "log": {
        "disabled": false,
        "level": "info",
        "timestamp": true
    },
    "inbounds": [
        {
            "type": "mixed",
            "listen": "127.0.0.1",
            "listen_port": 1080,
            "sniff": true,
            "set_system_proxy": false
        }
    ],
    "outbounds": [
        {
            "type": "vmess",
            "tag": "vmess-out",
            "server": "${SERVER}",
            "server_port": ${SERVER_PORT},
            "uuid": "${UUID}",
            "security": "auto",
            "alter_id": 0,
            "global_padding": false,
            "authenticated_length": true,
            "network": "tcp",
            "tls": {
                "enabled": false
            },
            "packet_encoding": "",
            "multiplex": {
                "enabled": true,
                "max_connections": 4,
                "min_streams": 4
            }
        },
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "block",
            "tag": "block"
        }
    ],
    "route": {
        "rules": [
            {
                "geosite": "category-ads-all",
                "outbound": "block"
            },
            {
                "geosite": "cn",
                "geoip": "cn",
                "outbound": "direct"
            }
        ]
    }
}
